{
  "name": "webtoapk-studio-electron",
  "version": "1.0.0",
  "description": "Modern WEB to APK conversion studio built with Electron",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir",
    "package": "electron-packager . webtoapk-studio --platform=win32 --arch=x64 --out=output --overwrite"
  },
  "keywords": [
    "electron",
    "web",
    "apk",
    "android",
    "converter"
  ],
  "author": "WEBtoAPK Studio",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "archiver": "^6.0.0",
    "fs-extra": "^11.0.0"
  },
  "build": {
    "appId": "com.webtoapk.studio",
    "productName": "WEBtoAPK Studio",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*"
    ]
  }
}
